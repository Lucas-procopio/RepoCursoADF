{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-curso-udemy-prd"
		},
		"lsAzureSQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lsAzureSQL'"
		},
		"lsDatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsDatalake'"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer1'"
		},
		"lsAzureSQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-cursoudemy-prd.database.windows.net"
		},
		"lsAzureSQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-cursoudemy-prd"
		},
		"lsAzureSQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "lucas"
		},
		"lsDatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsudemyprd.dfs.core.windows.net/"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\lsprocopio\\Downloads\\filesTXT"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "lsprocopio"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-curso-udemy-prd_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsAzureSQL_password": {
						"value": "[parameters('lsAzureSQL_password')]"
					},
					"lsDatalake_accountKey": {
						"value": "[parameters('lsDatalake_accountKey')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"lsAzureSQL_properties_typeProperties_server": {
						"value": "[parameters('lsAzureSQL_properties_typeProperties_server')]"
					},
					"lsAzureSQL_properties_typeProperties_database": {
						"value": "[parameters('lsAzureSQL_properties_typeProperties_database')]"
					},
					"lsAzureSQL_properties_typeProperties_userName": {
						"value": "[parameters('lsAzureSQL_properties_typeProperties_userName')]"
					},
					"lsDatalake_properties_typeProperties_url": {
						"value": "[parameters('lsDatalake_properties_typeProperties_url')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-curso-udemy-prd_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-curso-udemy-prd_ArmTemplate_0')]"
			]
		}
	]
}